{
	"extends": "airbnb",
	"env": {
		"browser": true
	},
	"rules": {
		"no-throw-literal": 0,
		"react/sort-comp": 0,
		"class-methods-use-this": 0,
		"react/jsx-filename-extension": 0,
		"react/prop-types": 0,
		"no-use-before-define": 0,
		"indent": [2, "tab"],
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"],
		"no-tabs": 0,
		"react/prefer-stateless-function": 0
	}
}